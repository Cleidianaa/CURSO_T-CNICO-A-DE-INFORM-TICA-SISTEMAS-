class DemoImpressao{
	public static void main(String[] args)// Estava faltando declarar o método - Público , Estático e nulo. 
		{
			System.out.println("7+2="+(7+2));
			System.out.println("7-2="+(7-2));
			System.out.println("7*2="+(7*2));
			System.out.println("7/2="+(7/2));
			return;
		}//não precisa do -True.

}//fim da classe